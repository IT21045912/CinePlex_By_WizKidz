<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/bg"
    android:layout_height="match_parent"
    tools:context=".BookingHome">

    <TextView
        android:id="@+id/feedbackheader1"
        android:layout_width="187dp"
        android:layout_height="44dp"
        android:layout_marginTop="64dp"
        android:text="My Booking "
        android:textColor="#101010"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/feedbackimage"
        android:layout_width="422dp"
        android:layout_height="333dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/feedbackheader1"
        app:layout_constraintVertical_bias="0.096"
        app:srcCompat="@drawable/feedbackimage" />

    <Button
        android:id="@+id/add_rating"
        android:layout_width="291dp"
        android:layout_height="45dp"
        android:backgroundTint="@color/blue"
        android:onClick="goToAddFeedback"
        android:text="Add New Booking"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:cornerRadius="?android:attr/windowTitleSize"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/feedbackimage"
        app:layout_constraintVertical_bias="0.423" />

    <Button
        android:id="@+id/view_all_ratings"
        android:layout_width="291dp"
        android:layout_height="45dp"
        android:backgroundTint="@color/blue"
        android:onClick="goToMyRatings"
        android:text="View My Bookings"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:cornerRadius="?android:attr/windowTitleSize"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.765" />
</androidx.constraintlayout.widget.ConstraintLayout>